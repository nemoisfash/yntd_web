<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.hxz.webapp.content.mapper.RelatedFileMapper">

	<!-- 单表查询映射结果 -->
	<resultMap id="BaseResultMap" type="cn.hxz.webapp.content.entity.RelatedFile">
		<id column="id" property="id" />
		<result column="title" property="title" />
		<result column="intro" property="intro" />
		<result column="filename" property="filename" />
		<result column="enabled" property="enabled" />
		<result column="trashed" property="trashed" />
		<result column="priority" property="priority" />
		<result column="channel_id" property="channelId" />
		<result column="create_by" property="createBy" />
		<result column="create_time" property="createTime" />
		<result column="update_by" property="updateBy" />
		<result column="update_time" property="updateTime" />
		<result column="hits" property="hits" />
	</resultMap>

    <!-- 单表查询结果列 -->
    <sql id="Base_Column_List">
        id, title, intro, filename, enabled, trashed, priority, channel_id AS channelId, create_by AS createBy, create_time AS createTime, update_by AS updateBy, update_time AS updateTime, hits
    </sql>
</mapper>
