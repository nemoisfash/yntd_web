<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.hxz.webapp.content.mapper.ModelMapper">

	<!-- 单表查询映射结果 -->
	<resultMap id="BaseResultMap" type="cn.hxz.webapp.content.entity.Model">
		<id column="id" property="id" />
		<result column="name" property="name" />
		<result column="builtin" property="builtin" />
		<result column="enabled" property="enabled" />
		<result column="priority" property="priority" />
		<result column="parent_id" property="parentId" />
	</resultMap>

    <!-- 单表查询结果列 -->
    <sql id="Base_Column_List">
        id, name, builtin, enabled, priority, parent_id AS parentId
    </sql>
</mapper>
